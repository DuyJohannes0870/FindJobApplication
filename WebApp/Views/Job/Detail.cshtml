                                        case JobType.FullTime:
                                            <span>FullTime</span>
                                            break;
                                        case JobType.Freelancer:
                                            <span>Freelancer</span>
                                            break;
                                        case JobType.PartTime:
                                            <span>PartTime</span>
                                            break;
                                        case JobType.Internship:
                                            <span>Internship</span>
                                            break;
                                    }
                                }
                            </li>
                            <li>Salary : <span>$@Decimal.Floor(@Model.StartSalary) - @Decimal.Floor(@Model.StartSalary) per month</span></li>
                            <li>Experience required : 
                                @{
                                    switch  (@Model.SkillLevel)
